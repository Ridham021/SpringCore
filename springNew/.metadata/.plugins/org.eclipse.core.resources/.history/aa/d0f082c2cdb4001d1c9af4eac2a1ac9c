package com.ridham.autowired;

import org.springframework.beans.factory.annotation.Autowired;

public class Human {

	private Heart heart;

	@Autowired
	public Human() {
		this.heart=heart;
	}
	
	public void setHeart(Heart heart) {
		this.heart = heart;
	}


	public void StartPumping() {
		
		heart.pump();
	}
	

}
